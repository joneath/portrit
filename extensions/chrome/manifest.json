{
    "name": "Portrit Photo Sharing Extension",
    "version": "1.0.0",
    "description": "Quickly share the best photos on the web with the Portrit Chrome extension.",
    "icons": { 
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
	"background_page": "background.html",
    "permissions": [
		"http://www.portrit.com/",
	    "contextMenus",
	    "tabs",
	    "http://*/*",
	    "https://*/*"
    ]
}